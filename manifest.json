{
  "name": "WebCat",
  "version": "1.0",
  "manifest_version": 2,
  "description": "WebCat",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "browser_action": {
    "default_icon": "images/icon.png",
	"default_popup": "popup.html"
  },
  "permissions": [
	"<all_urls>",
	"bookmarks",
	"tabs"
  ]
}
